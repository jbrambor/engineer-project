{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.1",
        "@testing-library/react": "^12.1.2",
        "@testing-library/user-event": "^13.5.0",
        "node-sass": "^7.0.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-scripts": "5.0.0",
        "sass-loader": "^12.6.0",
        "styled-components": "^5.3.3",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "start:dev": "set BROWSER=none&& react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress": "start-server-and-test start:dev http://localhost:3000 cypress:open",
        "cypress:dev": "start-server-and-test start:dev http://localhost:3000 cypress:run",
        "lint": "eslint --color ./src",
        "lint:fix": "eslint --fix ./src",
        "format": "prettier --write ./src",
        "format:check": "prettier --check ./src",
        "prepare": "cd ../ && husky install ./frontend/.husky"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.17.0",
        "@babel/preset-react": "^7.16.7",
        "@testing-library/cypress": "^8.0.2",
        "cypress": "^9.4.1",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.0.0",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-react": "^7.28.0",
        "eslint-plugin-react-hooks": "^4.3.0",
        "eslint-plugin-testing-library": "^5.0.5",
        "husky": "^7.0.4",
        "lint-staged": "^12.3.3",
        "prettier": "^2.5.1",
        "react-router-dom": "^6.2.2",
        "sass": "^1.49.9",
        "start-server-and-test": "^1.14.0"
    }
}
